Package: twopartm
Type: Package
Title: Two-Part Model with Marginal Effects
Version: 0.1.0
Authors@R: c(person("Yajie", "Duan", email = "yajieritaduan@gmail.com", role = c("aut","cre")),
  person("Birol", "Emir", email = "birol.emir@pfizer.com", role = "aut"),
